{
  "name": "Chrome Github Trending Sidebar",
  "version": "0.1.0",
  "description": "Example chrome sidebar extension to embed an iframe.",
  "web_accessible_resources": ["entry.js"],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Chrome Github Trending Sidebar"
  },
  "manifest_version": 2,
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ],
  "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "windows": "Ctrl+/",
          "mac": "Command+/",
          "chromeos": "Ctrl+/",
          "linux": "Ctrl+/"
        }
      }
   }
}
